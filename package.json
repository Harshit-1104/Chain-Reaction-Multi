  "bugs": {
    "url": "https://github.com/Harshit-1104/Chain-Reaction-Multi/issues"
  },
  "homepage": "https://github.com/Harshit-1104/Chain-Reaction-Multi#readme",
  "dependencies": {
    "express": "^4.17.1",
    "socketio": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "node-sass": "^6.0.1"
  }
}
